#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dyuzhev/Projects/Pet/it-incubator/sprints/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.10.6_ts-node@10.9.2/node_modules/create-jest/bin/node_modules:/Users/dyuzhev/Projects/Pet/it-incubator/sprints/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.10.6_ts-node@10.9.2/node_modules/create-jest/node_modules:/Users/dyuzhev/Projects/Pet/it-incubator/sprints/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.10.6_ts-node@10.9.2/node_modules:/Users/dyuzhev/Projects/Pet/it-incubator/sprints/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dyuzhev/Projects/Pet/it-incubator/sprints/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.10.6_ts-node@10.9.2/node_modules/create-jest/bin/node_modules:/Users/dyuzhev/Projects/Pet/it-incubator/sprints/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.10.6_ts-node@10.9.2/node_modules/create-jest/node_modules:/Users/dyuzhev/Projects/Pet/it-incubator/sprints/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.10.6_ts-node@10.9.2/node_modules:/Users/dyuzhev/Projects/Pet/it-incubator/sprints/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../bin/create-jest.js" "$@"
fi
